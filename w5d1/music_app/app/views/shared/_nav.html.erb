<header>
  <nav>
    <ul class='session-control-buttons'>
      <% if logged_in? %>
        <li><%= current_user.email %></li>
        <li><%= button_to 'Log Out', :session, method: :delete %></li>
      <% else %>
        <li><%= link_to 'Log In', new_session_url %></li>
        <li><%= link_to 'Sign Up', new_user_url %></li>
      <% end %>
    </ul>
  </nav>
</header>
